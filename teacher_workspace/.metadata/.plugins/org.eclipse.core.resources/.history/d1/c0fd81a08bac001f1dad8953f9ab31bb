package forTest;
//12번 : for문 연습
public class ForTest03 {
	public static void main(String[] args) {
		
		//이름 5번을 출력
//		for(초기식; 조건식; 증감식) {
//			syso(번 이름);
//		}
		
		//1) for( 초기식 int i = 1; 조건식 i <= 5; i++)
		//2) { 출력(i + "번 이름");}
		
		for(int i = 0; i < 5; i++) {
			System.out.println(i + "번 김영선");
		}
		
	}
}
