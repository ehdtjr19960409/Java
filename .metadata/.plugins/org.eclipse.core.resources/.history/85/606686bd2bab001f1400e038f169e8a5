package task;

public class Task03 {
   public static void main(String[] args) {
	   int intNum = 100;
	   long longNum = 200;
	   double doubleNum = 0.34;
	   float floatNum = 1;
	   char charNum = 'd';
	   
	   
	   //int, long, double, float, boolean, char
      //1. 정수(int) -> 모든 자료형 강제형변환 가능여부 확인
//	   int result1 = (long)intNum;
//	   int result2 = (double)intNum;
//	   int result3 = (float) floatNum;
//	   int result4 = (char) charNum;

	   
      //2. 정수(long) -> 모든 자료형 강제형변환 가능여부 확인
       //int result4 = (long) longNum;

	   
      //3. 실수(double) -> 모든 자료형 강제형변환 가능여부 확인
	   double result8 = (int) doubleNum;
	   double result9 = (long) doubleNum;
	   double result10 = (float) doubleNum;
//	   double result11 = (boolean) doubleNum;
	   double resutl12 = (char) doubleNum;
      
	   //4. 실수(float) -> 모든 자료형 강제형변환 가능여부 확인
       float result12 = (int)floatNum;
       float result13 = (long)floatNum;
       
      //5. 논리형 (boolean) -> 모든 자료형 강제형변환 가능여부 확인
      
      //6. 문자형(char) -> 모든 자료형 강제형변환 가능여부 확인
   }
}