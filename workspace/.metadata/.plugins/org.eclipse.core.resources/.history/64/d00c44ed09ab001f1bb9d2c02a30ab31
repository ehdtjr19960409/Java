package task;

public class Task03 {
   public static void main(String[] args) {
	   int intNum = 100;
	   long longNum = 200;
	   double doubleNum = 300;
	   float floatNum = 400;
	   char charNum = 'd';
	   
	   
	   //int, long, double, float, boolean, char
      //1. 정수(int) -> 모든 자료형 강제형변환 가능여부 확인
	   long result = (long) intNum;
	   double result1 = (double) intNum;
	   float result2 = (float) intNum;
	   char result3 = (char) intNum;
	   
      //2. 정수(long) -> 모든 자료형 강제형변환 가능여부 확인
       //int result4 = (long) longNum;
	   double result5 = (long) longNum; 
	   float result6 = (long) longNum;
	   //char result7 = (long) longNum;
	   
      //3. 실수(double) -> 모든 자료형 강제형변환 가능여부 확인
	   double result8 = (int) doubleNum;
	   double result9 = (int) doubleNum;
//	   float result10 = (double) doubleNum;
//	   char result11 = (double) doubleNum;
      
	   //4. 실수(float) -> 모든 자료형 강제형변환 가능여부 확인
       float result12 = (int)floatNum;
       float result13 = (long)floatNum;
       
      //5. 논리형 (boolean) -> 모든 자료형 강제형변환 가능여부 확인
      
      //6. 문자형(char) -> 모든 자료형 강제형변환 가능여부 확인
   }
}